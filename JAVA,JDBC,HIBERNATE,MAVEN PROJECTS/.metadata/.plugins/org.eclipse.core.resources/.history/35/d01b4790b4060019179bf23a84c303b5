
public class SelectionSort {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]={38,27,5,6,45};
		int i,j;
		int min,temp=0;
		for(i=0;i<=a.length;i++);
		{
		    min=i;
			for(j=i+1;j<=a.length;j++)
			{
				if(a[j]<a[min])
				{
					min=j;
				}
			}
			temp=a[i];
			a[i]=a[min];
			a[min]=temp;
		}
		for(i=0;i<=a.length;i++)
		{
			System.out.println(a[i]+" ");
		}
	}

}
